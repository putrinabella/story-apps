{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "import/no-extraneous-dependencies": 0,
    "no-console": 0,
    "no-alert": 0,
    "no-undef": 0,
    "no-underscore-dangle": 0,
    "no-restricted-globals": 0,
    "linebreak-style": 0,
    "no-irregular-whitespace": "off",
    "import/prefer-default-export": "off",
    "no-unused-vars": "off",
    "class-methods-use-this": "off",
    "global-require": "off",
    "no-return-await": "off",
    "no-void": "off",
    "consistent-return": "error"
  }
}
